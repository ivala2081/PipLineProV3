# PipLinePro Environment Configuration
# Copy this file to .env and update the values

# Supabase PostgreSQL Database Configuration
DATABASE_URL=postgresql://postgres:[YOUR_PASSWORD]@db.sihlxucjplorgptrosed.supabase.co:5432/postgres

# Alternative: Individual PostgreSQL parameters
POSTGRES_HOST=db.sihlxucjplorgptrosed.supabase.co
POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=[YOUR_PASSWORD]
POSTGRES_SSL_MODE=require

# Application Configuration
FLASK_ENV=development
DEBUG=True
SECRET_KEY=dev-secret-key-change-in-production

# Admin User Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-admin-password
ADMIN_EMAIL=admin@pipeline.com

# Redis Configuration
REDIS_ENABLED=false

# Logging
LOG_LEVEL=INFO

# Database Migration
INIT_DB=true